{
  "env": {
    "browser": true,
    "es2021": true,
    "jasmine": true,
    "jquery": true,
    "commonjs": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "ignorePatterns": [
    "**/third-party/*.js"
  ],
  "globals": {
    "CSRF_HEADER_NAME": "readonly",
    "CSRF_TOKEN": "readonly",
    "GAPI_CLIENT_READY": "readonly"
  }
}
